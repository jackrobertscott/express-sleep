  "bugs": {
    "url": "https://github.com/jackrobertscott/express-sleep/issues"
  },
  "homepage": "https://github.com/jackrobertscott/express-sleep#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
