{
  "name": "hobson",
  "version": "0.4.2",
  "description": "Lightweight, minimalistic approach to fully functioning RESTful endpoints in express",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && babel src -d lib",
    "test": "npm run build && NODE_ENV=test mocha --exit",
    "test:watch": "npm run build && NODE_ENV=test mocha --watch",
    "release": "np",
    "prepublish": "npm run build",
    "dev": "nodemon --exec \"npm run build && node use/index.js\" --watch src --watch use --delay 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jackrobertscott/hobson.git"
  },
  "keywords": [
    "express",
    "rest",
    "restful",
    "boilerplate",
    "starter",
    "mongoose"
  ],
  "author": "Jack Scott <jack.rob.scott@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jackrobertscott/hobson/issues"
  },
  "homepage": "https://github.com/jackrobertscott/hobson#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.18.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "faker": "^4.1.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1",
    "np": "^2.20.1",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "chalk": "^2.3.1",
    "change-case": "^3.0.1",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.0",
    "express": "^4.16.2",
    "helmet": "^3.11.0",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.1.1",
    "method-override": "^2.3.10",
    "mongoose": "^5.0.6",
    "morgan": "^1.9.0",
    "pluralize": "^7.0.0"
  }
}
