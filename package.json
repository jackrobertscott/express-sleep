{
  "name": "hobson",
  "version": "0.1.0",
  "description": "Convention over configuration approach to RESTful endpoints using express.",
  "main": "src/index.js",
  "engines": {
    "node": ">=7.6"
  },
  "scripts": {
    "clean": "rm -rf lib && mkdir lib",
    "build": "npm run clean && babel src -d lib",
    "test": "npm run build && mocha --exit",
    "test:watch": "npm run build && mocha --watch",
    "release": "np",
    "prepublish": "npm run build",
    "dev": "NODE_ENV=development nodemon use/index.js --watch src --watch use"
